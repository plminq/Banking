# Counterfactual Financial Oracle - Environment Variables
# Copy this file to .env and fill in your values

# Required API Keys
GEMINI_API_KEY=your_gemini_api_key_here
DEEPSEEK_API_KEY=your_deepseek_api_key_here
LANDINGAI_API_KEY=your_landing_ai_api_key_here

# Database (Railway will auto-populate this)
DATABASE_URL=postgresql://user:password@host:5432/dbname

# CORS Origins (comma-separated, no spaces)
CORS_ORIGINS=http://localhost:3000,http://localhost:5173

# Frontend API URL (set to your deployed backend URL in production)
VITE_API_URL=http://localhost:8000
